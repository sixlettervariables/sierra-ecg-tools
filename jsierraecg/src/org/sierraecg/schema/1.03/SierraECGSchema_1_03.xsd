	<xsd:element name = "scmodifiers">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name = "scmodifier" minOccurs = "0">
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base = "xsd:string">
								<xsd:attribute name = "scnumericcode"  use = "required" type = "xsd:nonNegativeInteger"/>	
								<xsd:attribute name = "scmodifiercode" use = "required" type = "xsd:string"/>
								<xsd:attribute name = "added"   type = "TYPEflag"/>
								<xsd:attribute name = "deleted" type = "TYPEflag"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
