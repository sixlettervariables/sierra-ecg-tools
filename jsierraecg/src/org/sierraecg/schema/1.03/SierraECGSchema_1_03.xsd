								<xsd:attribute name = "modifiercode" use = "required" type = "xsd:string"/>
								<xsd:attribute name = "added"   type = "TYPEflag"/>
								<xsd:attribute name = "deleted" type = "TYPEflag"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name = "changed" default = "False" type = "TYPEflag"/>
		</xsd:complexType>
